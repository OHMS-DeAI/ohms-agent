{
  "canisters": {
    "ohms_agent": {
      "type": "rust",
      "package": "ohms_agent",
      "candid": "src/ohms_agent.did"
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": "-C link-arg=-Wl,--no-undefined-version"
    },
    "canister_http": {
      "enabled": false
    },
    "rust": {
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "type": "persistent"
    }
  },
  "version": 1
}